 {
    "name": "Simplify",
    "version": "1.0",
    "description": "Build an Extension!",
    "content_scripts": [
	  {
	    "matches": ["https://*/*", "http://*/*"],
	    "js": ["jquery-3.5.1.min.js", "content.js"]
	  }
	],
	"browser_action": {
	  "default_popup": "popup.html"
	  //"default_icon": "icon.png"
	},
	"permissions": ["https://*/*", "http://*/*"],
    "manifest_version": 2
  }